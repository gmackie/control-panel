# OAuth token for accessing protected services
OAUTH_TOKEN=your-oauth-token-here

# Stripe API key for payment data
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key

# Clerk API key for user management
CLERK_SECRET_KEY=sk_test_your-clerk-secret-key

# Turso API token for database access
TURSO_API_TOKEN=your-turso-api-token

# K8s service account token
NEXT_PUBLIC_K8S_TOKEN=your-k8s-token

# Service URLs (optional, defaults are set in next.config.js)
PROMETHEUS_URL=http://prometheus.monitoring.svc.cluster.local:9090
GRAFANA_URL=https://monitoring.gmac.io